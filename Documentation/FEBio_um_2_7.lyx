\end_layout

\begin_layout LyX-Code
    <param name="mat1.E">1, 0, 5</param>
\end_layout

\begin_layout LyX-Code
    <param name="mat1.v">-0.5, 0, 0.5</param>
\end_layout

\begin_layout LyX-Code
