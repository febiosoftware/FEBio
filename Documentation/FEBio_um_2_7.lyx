\end_layout

\begin_layout LyX-Code

\change_inserted 2145056167 1496352061
	<beta [lc="1"]>1</beta>
\end_layout

\begin_layout LyX-Code

\change_inserted 2145056167 1496352442
</surface_load>
\end_layout

