\end_layout

\begin_layout LyX-Code
<material id="1" type="porous neo-Hookean">
\end_layout

\begin_layout LyX-Code
  <density>1.0</density>
\end_layout

\begin_layout LyX-Code
  <E>1.0</E>
\end_layout

\begin_layout LyX-Code
  <phi0>0.2</phi0>
\end_layout

\begin_layout LyX-Code
</material>
