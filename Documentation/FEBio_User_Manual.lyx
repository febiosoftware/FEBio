	<mu>1</mu>
\end_layout

\begin_layout LyX-Code
	<tauy>40</tauy>
\end_layout

\begin_layout LyX-Code
	<n>40</n>
\end_layout

\begin_layout LyX-Code
</viscous>
\end_layout

