         <scale lc="2">1.0</scale>
\end_layout

\begin_layout LyX-Code
      </prescribe>
\end_layout

\begin_layout LyX-Code
    </Boundary>
\end_layout

\begin_layout LyX-Code
  </Step>
\end_layout

\begin_layout LyX-Code
  <LoadData>
