  <Boundary>
\end_layout

\begin_layout LyX-Code
    <!-- global boundary conditions -->
\end_layout

\begin_layout LyX-Code
  </Boundary>
\end_layout

\begin_layout LyX-Code
  <LoadData>
\end_layout

\begin_layout LyX-Code
    <!-- load curve data goes here -->
\end_layout

\begin_layout LyX-Code
  </LoadData>
\end_layout

\begin_layout LyX-Code
  
\series bold
<Steps>
\end_layout

\begin_layout LyX-Code

